<odoo>
    <data noupdate="1">
        <record id="sequence_dev_model" model="ir.sequence">
            <field name="name">Development Model</field>
            <field name="code">dev.model</field>
            <field name="prefix">DEV-MOD/%(year)s/</field>
            <field eval="1" name="number_next" />
            <field eval="1" name="number_increment" />
            <field name="padding" eval="6" />
            <field name="use_date_range" eval="1" />
            <field eval="False" name="company_id" />
        </record>
    </data>
</odoo>
